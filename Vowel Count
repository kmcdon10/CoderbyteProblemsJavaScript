// Directions: Have the function VowelCount(str) take the str string parameter being passed and return the number of 
// vowels the string contains (ie. "All cows eat grass" would return 5). Do not count y as a vowel for this challenge. 

function VowelCount(str) { 
	var count = 0;
  	for (var i = 0; i < str.length; i++){
      switch(str.charAt(i)){
          case 'a':
          	count++;
          	break;
          case 'e':
          	count++;
          	break;
          case 'i':
          	count++;
          	break;
          case 'o':
          	count++;
          	break;
          case 'u':
          	count++;
          	break;
        default:
			// do nothing. Letter is not a vowel
      }
    }
  
  return count;     
};
   
// keep this function call here 
// to see how to enter arguments in JavaScript scroll down
VowelCount(readline());                            



